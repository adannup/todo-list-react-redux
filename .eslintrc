{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true
  },
  "rules": {
    "linebreak-style": ["error", "windows"],
    "react/jsx-filename-extension": [1, {"extensions": [".js"]}],
    "arrow-parens": ["error", "as-needed"],
    "import/no-extraneous-dependencies": [error, { devDependencies: true }]
  }
}
